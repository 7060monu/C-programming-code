#include<stdio.h>
void main(){
    int a[3][3],b[10][10],i,j;
    printf(" enter the elemnt of array :");
     for(int i=0;i<3;i++){
        for(j=0;j<3;j++){
            scanf("%d",&a[i][j]);
        }
    }
    printf("  matrix is :");
     for(int i=0;i<3;i++){
        for(j=0;j<3;j++){
            printf("%d\t",a[i][j]);
        }
        printf("\n");

    }
    for(i=1;i<3;i++){
        for(j=0;j<i;j++){
            b[i][j]=a[i][j];
        }
    }
    /*for(i=0;i<3;i++){
        for(j=0;j<3;j++){
            printf("%d\t",b[i][j]);
        }
        printf("\n");
    }*/
     for(i=1;i<3;i++){
        for(j=0;j<i;j++){
            printf("%d\t",b[i][j]);
        }
        printf("\n");


}
}
