#include<stdio.h>
void main(){
      int a[3][3],b[10][10],i,j,sum=0;
    printf(" enter the elemnt of array :");
     for(int i=0;i<3;i++){
        for(j=0;j<3;j++){
            scanf("%d",&a[i][j]);
        }
    }
    printf("  matrix is :");
     for(int i=0;i<3;i++){
        for(j=0;j<3;j++){
            printf("%d\t",a[i][j]);
        }
        printf("\n");
     }
           for(i=0;i<2;i++){
        for(j=i+1;j<3;j++){
            sum=sum+a[i][j];
        }
    }
    printf("%d",sum);
}
