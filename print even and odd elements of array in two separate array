#include<stdio.h>
void main(){
    int a[100],b[100],c[100],n;
    printf("enter the size of array :");
    scanf("%d",&n);
    printf("enter the  element of array :");
   int i,j;
   for(i=0;i<n;i++){
    scanf("%d",&a[i]);
   }
   int k=0;
   int m=0;
   for(i=0;i<n;i++){
    if(a[i]%2==0){
        b[k]=a[i];
        k++;
    }
    else{
        c[m]=a[i];
        m++;
    }
   }
   for(i=0;i<k;i++){
    printf("%d",b[i]);
   }
   for(i=0;i<m;i++){
    printf("%d",&c[i]);
   }
}
