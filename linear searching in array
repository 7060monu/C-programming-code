#include<stdio.h>
void main(){
    int a[100],n,i,b;
    printf("enter the size of array:");
    scanf("%d",&n);
    printf(" enter the element of array:");
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    printf("enter the element for searching :");
    scanf("%d",&b);
    for(i=0;i<n;i++){
        if(b==a[i]){
            printf(" yes it is prsent in array");
        }
    }

}
